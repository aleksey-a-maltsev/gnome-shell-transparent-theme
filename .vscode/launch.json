    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "gulp",
            "skipFiles": [
                "<node_internals>/**"
            ],
			"program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
			"stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy"
            ],
            "console": "internalConsole"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "prepare",
            "skipFiles": [
                "<node_internals>/**"
            ],
			"program": "${workspaceFolder}/cli/prepare.js",
			"stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeArgs": [
                "--nolazy"
            ],
            "console": "internalConsole"
        }
    ]
}
